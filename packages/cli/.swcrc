{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "es6",
    "resolveFully": true
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true,
      "decorators": true
    },
    "baseUrl": "./src",
    "paths": {
      "src/*": ["./src/*"]
    },    
    "transform": {},
    "target": "esnext",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  },
  "minify": false,
  "sourceMaps": "inline" 
}